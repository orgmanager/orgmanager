{
  "name": "orgmanager/orgmanager",
  "description": "Supercharge you GitHub organizations!",
  "keywords": [
    "orgmanager",
    "github",
    "organizations",
    "supercharge",
    "invite",
    "invites",
    "php",
    "laravel"
  ],
  "license": "MPL-2.0",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "fideloper/proxy": "4.0.0",
    "google/recaptcha": "1.1.3",
    "graham-campbell/github": "7.3.0",
    "guzzlehttp/guzzle": "6.3.3",
    "knplabs/github-api": "2.8.0",
    "laravel/framework": "5.6.28",
    "laravel/socialite": "3.0.12",
    "laravel/tinker": "1.0.7",
    "league/commonmark": "0.15.7",
    "orgmanager/orgmanager-logos": "1.1",
    "php-http/cache-plugin": "1.5.0",
    "php-http/guzzle6-adapter": "1.1.1",
    "sentry/sentry-laravel": "0.8.0"
  },
  "require-dev": {
    "filp/whoops": "2.4.1",
    "fzaninotto/faker": "1.8.0",
    "mockery/mockery": "1.2.2",
    "phpunit/phpunit": "7.5.14"
  },
  "autoload": {
    "classmap": ["database/seeds", "database/factories"],
    "files": ["app/helpers.php"],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": ["@php artisan key:generate"],
    "post-install-cmd": ["@php artisan migrate --force"],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover",
      "@php artisan migrate --force"
    ],
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
